{
  "name": "pecans",
  "version": "1.0.0",
  "description": "Pretty easy Two Cans & String API wrapper",
  "scripts": {
    "build": "jsii --strict",
    "dist": "jsii --strict && jsii-pacmak",
    "clean": "rm -r dist obj || true",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/humanoidsandvichdispenser/pew.git"
  },
  "main": "obj/index.js",
  "types": "obj/index.d.ts",
  "author": {
    "name": "HumanoidSandvichDispenser",
    "email": "humanoidsandvichdispenser@gmail.com"
  },
  "license": "GPL-3.0-only",
  "jsii": {
    "excludeTypescript": [
      "**/*.test.ts"
    ],
    "versionFormat": "full",
    "outdir": "dist",
    "tsc": {
      "outDir": "obj",
      "rootDir": "src"
    },
    "targets": {
      "python": {
        "distName": "pecans",
        "module": "pecans"
      },
      "dotnet": {
        "namespace": "Pecans",
        "packageId": "Pecans"
      }
    }
  },
  "stability": "experimental",
  "devDependencies": {
    "@types/node": "^20.8.4",
    "jsii": "^5.2.14",
    "jsii-pacmak": "^1.90.0",
    "vitest": "^0.34.6"
  }
}
